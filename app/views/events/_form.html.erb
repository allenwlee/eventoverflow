<%= form_for @event do |f| %>
  <%= f.text_field(:title, :placeholder =>  "Title") %>
  <%= f.text_area :description, :placeholder => "Description" %>
  <%= f.datetime_select :starts_at %>
  <%= f.text_field :duration, :placeholder => "Duration" %>
  <%= f.text_area :location, :placeholder => "Location" %>
  <%= f.submit %>

<% end %>
